{
    "version": 2,
    "name": "nodefaas",
    "alias": [
        "rynodefaas"
    ],
    "env": {
        "pg_user_faas": "@pg_user_faas",
        "pg_host_faas": "@pg_host_faas",
        "pg_dbname_faas": "@pg_dbname_faas",
        "pg_password_faas": "@pg_password_faas",
        "cookie_secret": "@faas_cookie_sign"
    },
    "builds": [{
        "src": "backend/*.js",
        "use": "@now/node"
    }, {
        "src": "package.json",
        "use": "@now/static-build",
        "config": {
            "distDir": "dist"
        }
    }],
    "routes": [{
            "src": "/api/signup",
            "dest": "/backend/signup.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/createTransaction",
            "dest": "/backend/createTransaction.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/login",
            "dest": "/backend/login.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/createAccount",
            "dest": "/backend/createAccount.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/echo",
            "dest": "/backend/echo.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/(.*)",
            "dest": "/backend/$1.js",
            "methods": ["GET"]
        }, {
            "src": "/me",
            "dest": "/index.html"
        },
        {
            "src": "/account",
            "dest": "/index.html"
        },
        {
            "src": "/*",
            "dest": "/index.html"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ],
    "public": true
}